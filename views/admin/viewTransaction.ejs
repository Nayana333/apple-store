    <main class="main-wrap">
        
        <%- include("../partials/header2")%>

        <!-- Your form code -->
        
<!-- Rest of your EJS code -->

        
                <table class="table align-middle table-nowrap mb-0 ms-4">
                    <!-- Table header -->
                    <thead class="table-light"> 
                        <tr>                                   
                            <th class="align-middle" scope="col">USER</th>                          
                            <th class="align-middle" scope="col">AMOUNT</th>
                            <th class="align-middle" scope="col">TYPE</th>
                            <th class="align-middle" scope="col">PAYMENT METHOD</th>
                            <th class="align-middle" scope="col">DATE</th>
                        </tr>
                    </thead>
                    <!-- Table body -->
                    <tbody>
                        <% if (transaction.length > 0) { %>
                            <% for (let i = 0; i < transaction.length; i++) { %>
                                <tr>
                                    <td><%= transaction[i].user.name %></td> 
                                    <td><%= transaction[i].amount %></td>
                                    <td><%= transaction[i].type %></td>
                                    <td><%= transaction[i].paymentMethod %></td>
                                    <td><%= transaction[i].date.toDateString() %></td>
                                </tr>
                            <% } %>
                        <% } else { %>
                            <tr>
                                <td colspan="6">No transactions found</td>
                            </tr>
                        <% } %>
                    </tbody>
                </table>
                
        
        <!-- Pagination links -->


        <% if (totalPages > 1) { %>
            <!-- Pagination -->
            <nav aria-label="Page navigation">
                <ul class="pagination justify-content-center">
                    <% if (currentPage > 1) { %>
                        <li class="page-item">
                            <a class="page-link text-success" href="/admin/transaction?page=<%= currentPage - 1 %>" aria-label="Previous">Previous</a>
                        </li>
                    <% } %>
        
                    <% for (let page = 1; page <= totalPages; page++) { %>
                        <li class="page-item <%= page === currentPage ? 'active' : '' %>">
                            <a class="page-link text-success" href="/admin/transaction?page=<%= page %>"><%= page %></a>
                        </li>
                    <% } %>
        
                    <% if (currentPage < totalPages) { %>
                        <li class="page-item">
                            <a class="page-link text-success" href="/transaction?page=<%= currentPage + 1 %>" aria-label="Next">Next</a>
                        </li>
                    <% } %>
                </ul>
            </nav>
        <% } %>
        
          


