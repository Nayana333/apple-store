<body>
    <div class="screen-overlay"></div>
    <%- include("../partials/aside")%>
    
    <main class="main-wrap">
        <%- include("../partials/header2")%>
       
        <section class="content-main" style="margin-left: 100px;">
            <form method="post" action= " " enctype="multipart/form-data">
                <div class="content-header">
                    <h2 class="content-title ms-7">Add New Banner</h2>
                </div>
                <div class="col-lg-3">
                    <div class="card mb-4" style="width: 647px;">
                        <div class="card-header">
                            <h4>Banner Image</h4>
                        </div>
                        <div class="card-body">
                            <div class="input-upload">
                                <img src="assetsb/imgs/theme/upload.svg" alt="">
                                <input type="file" name="image" id="image" class="form-control" accept="image/*">
                            </div>
                        </div>
                    </div>
                </div>
            
                <div class="row">
                    <div class="col-lg-6">
                        <div class="card mb-4">
                            <div class="card-header">
                                <h4>Basic</h4>
                            </div>
                            <div class="card-body">
                                <div class="mb-4">
                                    <label class="form-label">Select Banner Type</label>
                                    <select class="form-select" name="bannerType">
                                        <option value="category Banner">Category Offer</option>
                                        <option value="product Banner">Product Offer</option>
                                        <option value="new Arrival">New Arrival</option>
                                        <option value="Deals and Promotions">Deals and Promotions</option>
                                        <option value="Seasonal Sales">Seasonal Sales</option>
                                    </select>
                                </div>
                                <div class="mb-4">
                                    <label class="form-label">Title</label>
                                    <input type="text" placeholder="" class="form-control" name="title">
                                </div>
                                <div class="mb-4">
                                    <label class="form-label">Link</label>
                                    <input type="text" placeholder="" class="form-control" name="link">
                                </div>
                                <div class="mb-4">
                                    <label class="form-label">Description</label>
                                    <input type="text" placeholder="" class="form-control" name="description">
                                </div>
                                <div class="mb-4">
                                    <label class="form-label">Offer</label>
                                    <input type="text" placeholder="" class="form-control" name="offer">
                                </div>
                                <div class="mb-4">
                                    <label class="form-label">Subtitle</label>
                                    <input type="text" placeholder="" class="form-control" name="subtitle">
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-6">
                        <div class="card mb-4" style="height: 583px;">
                            <div class="card-header">
                                <h4>Additional Details</h4>
                            </div>
                            <div<label class="form-label">Select Category</label>
                            <select class="form-select" name="category">
                                <% category.forEach(category => { %>
                                <option value="<%= category._id %>">
                                    <%= category.category %>
                                </option>
                                <% }) %>
                            </select><br>
                            
                            <label class="form-label">Select Product</label>
                            <select class="form-select" name="product">
                                <% product.forEach(product => { %>
                                <option value="<%= product._id %>">
                                    <%= product.name %>
                                </option>
                                <% }) %>
                            </select>
                            
                                <div class="mb-4">
                                    <label class="form-label">Start Date</label>
                                    <input type="date" placeholder="" class="form-control" name="startDate">
                                </div>
                                <div class="mb-4">
                                    <label class="form-label">End Date</label>
                                    <input type="date" placeholder="" class="form-control" name="endDate">
                                </div>
                                <input type="submit" class="btn btn-md rounded font-sm hover-up" value="Add Banner">
                            </div>
                        </div>
                    </div>
                </div>
            </form>
            
        </section> <!-- content-main end// -->
        <footer class="main-footer font-xs">
            <div class="row pb-30 pt-15">
                <div class="col-sm-6">
                    <script>
                    document.write(new Date().getFullYear())
                    </script> Â©, Evara - HTML Ecommerce Template .
                </div>
                <div class="col-sm-6">
                    <div class="text-sm-end">
                        All rights reserved
                    </div>
                </div>
            </div>
        </footer>
    </main>
    <script src="assetsb/js/vendors/jquery-3.6.0.min.js"></script>
    <script src="assetsb/js/vendors/bootstrap.bundle.min.js"></script>
    <script src="assetsb/js/vendors/select2.min.js"></script>
    <script src="assetsb/js/vendors/perfect-scrollbar.js"></script>
    <script src="assetsb/js/vendors/jquery.fullscreen.min.js"></script>
    <!-- Main Script -->
    <script src="assetsb/js/main.js" type="text/javascript"></script>
</body>


