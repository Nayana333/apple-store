
<layoutes></layoutes>/header.ejs') %>

<h1>Edit user</h1>


<form action="" method="POST">

    <input type="name" name="name" required placeholder="Enter name" value="<%= user.name %>" ><br>
    <br>
    <input type="email" name="email"  required placeholder="Enter email" value="<%= user.email %>" ><br>
    <br>
    <input type="mobile" name="mno"  required placeholder="Enter Mobile" value="<%= user.mobile %>" ><br>
    <br>
    <label>Verified</label>
    <input type="radio" name="verify" value="1"  <% if(user.is_verified == 1){ %>checked <% } %> >
    <label>Unverified</label>
    <input type="radio" name="verify" value="0" <% if(user.is_verified == 0){ %>checked <% } %> >
    <input type="submit" value="Update" ><br>
    <br>
    <input type="hidden" name5="id" value="<%= user._id %>">
</form>
<a href="/admin/dashboard/">Go dashboard</a>

<% include('../layoutes/footer.ejs') %>













