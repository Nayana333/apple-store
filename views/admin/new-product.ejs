<head>
    <meta charset="utf-8">
    <title>AppleStore</title>
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta property="og:title" content="">
    <meta property="og:type" content="">
    <meta property="og:url" content="">
    <meta property="og:image" content="">
    <!-- Favicon -->
    <link rel="shortcut icon" type="image/x-icon" href="assetsb/imgs/theme/favicon.svg">
    <!-- Template CSS -->
    <link href="assetsb/css/main.css" rel="stylesheet" type="text/css" />
</head>

<body>
    <div class="screen-overlay"></div>
    <%- include("../partials/aside")%>
    <main class="main-wrap">
        <%- include("../partials/header2")%>

        <section class="content-main">
            <form name="productform" id="productform" action="/admin/new-product" method="POST" enctype="multipart/form-data">
            <div class="row">
                <div class="col-9">
                    <div class="content-header">
                        <h2 class="content-title">Add New Product</h2>
                        
                    </div>
                </div>
              
                <div class="col-lg-6">
                    <div class="card mb-4" style="padding-bottom: 58px;">
                        <div class="card-body">
                            <h5 class="my-2">Product Info</h5>
                            <% if (typeof message !== 'undefined' && message) { %>
                                <p style="color: rgb(0, 125, 209);" id="message"><%= message %></p>
                            <% } %><br>
                            <div class="mb-4">
                                <label for="product_title" class="form-label">Product title</label>
                                <input type="text" placeholder="Type here" class="form-control" name="name" id="name">
                                <div class="error"></div>
                            </div>
                            <div class="row gx-3">
                               
                                <div class="  mb-3">
                                    <label for="product_size" class="form-label">Quanity</label>
                                    <input type="number" placeholder="Type here" class="form-control" name="quantity">
                                    <div class="error"></div>
                                </div>
                            </div>
                          
                            <div class="mb-4">
                                <label for="product_brand" class="form-label">product Color</label>
                                <input type="text" placeholder="Type here" class="form-control" name="productColor">
                                <div class="error"></div>
                            </div>
                        </div>
                    </div> <!-- card end// -->
                    <div class="card mb-4">
                        <div class="card-body">
                            <h5 class="my-2">Product Specifications</h5>
                            
                            <div class="row gx-3 ">
                                <div class="  mb-3  col-lg-6">
                                    <label for="product_title" class="form-label">Battery</label>
                                    <input type="text" placeholder="Type here" class="form-control" name="battery">
                                </div>
                               
                                <div class="  mb-3  col-lg-6">
                                    <label for="product_size" class="form-label">Ram</label>
                                    <input type="text" placeholder="Type here" class="form-control" name="ram">
                                </div>
                            </div>
                             
                            <div class="row gx-3 ">
                                <div class="  mb-3  col-lg-6">
                                    <label for="product_title" class="form-label">Rom</label>
                                    <input type="text" placeholder="Type here" class="form-control" name="rom">
                                </div>
                               
                                <div class="  mb-3  col-lg-6">
                                    <label for="product_size" class="form-label">Expandable</label>
                                    <input type="text" placeholder="Type here" class="form-control" name="expandable">
                                </div>
                            </div>
                             
                            <div class="row gx-3 ">
                                <div class="  mb-3  col-lg-6">
                                    <label for="product_title" class="form-label">RearCam</label>
                                    <input type="text" placeholder="Type here" class="form-control" name="rearCam">
                                </div>
                               
                                <div class="  mb-3  col-lg-6">
                                    <label for="product_size" class="form-label">FrontCam</label>
                                    <input type="text" placeholder="Type here" class="form-control" name="frontCam">
                                </div>
                            </div>
                          
                            <div class="mb-4">
                                <label for="product_brand" class="form-label">Processor</label>
                                <input type="text" placeholder="Type here" class="form-control" name="processor">
                            </div>
                        </div>
                    </div> 
                </div>
            
                <div class="col-lg-3" style="max-width: 50% !important;width: 50% !important;">
                    <div class="card mb-4">
                        <div class="card-body">
                            <div class="mb-4">
                                <label class="form-label">Price</label>
                                <input type="text" placeholder="Type here" class="form-control" name="price">
                                <div class="error"></div>
                            </div>
                            <div class="mb-4">
                                <label class="form-label">Discount Price</label>
                                <input type="text" placeholder="Type here" class="form-control" name="discountPrice">
                                <div class="error"></div>
                            </div>
                          
              
                            <hr>
                            <h5 class="mb-3">Categories</h5>
                            <div class="col-sm-9">
                                <label class="form-label">Select Categories</label>
                                <select class="form-select">
                                    <% categories.forEach(category=>{%>
                                        <option value="<%=category._id%>">
                                            <%= category.category %>
                                        </option>
                                        <% }) %>

                               
                                    
                                </select>

                               

                               
                                    
                                
                            
                            
                            
                         
                        </div>
                       
                    </div> <!-- card end// -->
         
                    <div class="card mb-4">
                        <div class="card-body">
                            <div>
                                <label class="form-label">Images</label>
                                <label for="productImages">Product Images (up to 4 images):</label>
                                <input type="file" id="productImages" name="productImages" multiple accept="image/jpeg, image/png" >
                                <br>
                            </div>
                        </div>
                    </div> <!-- card end// -->
                    
                    <div>
                           
                        <input type="submit" class="btn btn-md rounded font-sm hover-up" value="Create Product">
                    </div>
                </div>
               
            </div>
            
        </form>
        </section>
        <footer class="main-footer font-xs">
            <div class="row pb-30 pt-15">
                <div class="col-sm-6">
                    <script>
                    document.write(new Date().getFullYear())
                    </script> Â©, Evara - HTML Ecommerce Template .
                </div>
                <div class="col-sm-6">
                    <div class="text-sm-end">
                        All rights reserved
                    </div>
                </div>
            </div>
        </footer>
    </main>
    <script src="assetsb/js/vendors/jquery-3.6.0.min.js"></script>
    <script src="assetsb/js/vendors/bootstrap.bundle.min.js"></script>
    <script src="assetsb/js/vendors/select2.min.js"></script>
    <script src="assetsb/js/vendors/perfect-scrollbar.js"></script>
    <script src="assetsb/js/vendors/jquery.fullscreen.min.js"></script>
    <script src="assetsb/js/vendors/chart.js"></script>
    <!-- Main Script -->
    <script src="assetsb/js/main.js" type="text/javascript"></script>
    <script src="assetsb/js/custom-chart.js" type="text/javascript"></script>
</body>


