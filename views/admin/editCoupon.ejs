    <main class="main-wrap">
        <%- include("../partials/header2")%>
        <section class="content-main">
            <form action="/admin/editCoupon?_id=<%= coupon._id %> " method="post" >
            <div class="row justify-content-center">
                <!-- <h2 class="content-title">Add New category</h2> -->
                <div class="col-9">
                    <div class="content-header justify-content-center">
                        <!-- <h2 class="content-title">Add New category</h2> -->
                        
                
             
                <div class="col-lg-12">
                    <h2 class="content-title" style="margin-left: 15px;">EDIT COUPON</h2>
                    <h6 class="content-title" style="margin-left: 15px;">Coupon:<%= coupon.code %></h6>
                    <div class="card mb-4"  style="margin-top:15px">
                        <div class="card-body">
                            <% if (typeof message !== 'undefined' && message) { %>
                                <p style="color: rgb(0, 125, 209);" id="message"><%= message %></p>
                            <% } %>
                            <h5 class="my-2">Coupon Info</h5>
                            <input type="text" name="id" value="<%= coupon._id %>" style="display: none;"> 
                            <div class="mb-4">
                                <label for="product_title" class="form-label">coupon Code</label>
                                <input type="text" value="<%= coupon.code %>" id="couponCode" class="form-control" name="couponCode" required >
                                <div class="error"></div>
                            </div>
                            <div class="row gx-3">
                               
                                <div class="  mb-3">
                                    <label for="product_size" class="form-label">Discount(%)</label>
                                    <input type="number"  value="<%= coupon.discount %>" class="form-control" name="discount" id="discount" required>                                                                   
                                    <div class="error"></div>
                                </div>
                            </div>
                            <div class="row gx-3">
                               
                                <div class="  mb-3">
                                    <label for="product_size" class="form-label">Limit</label>
                                    <input type="number" value="<%= coupon.limit %>" class="form-control" name="limit" id="limit" required>
                                    <div class="error"></div>
                                </div>
                            </div>
                            <div class="row gx-3">
                               
                                <div class="  mb-3">
                                    <label for="product_size" class="form-label">Min Cart Amount</label>
                                    <input type="text" value="<%= coupon.minCartAmt %>" class="form-control" name="minCartAmt" id="minCartAmt">                                                                   
                                    <div class="error"></div>
                                </div>
                            </div>
                            <div class="row gx-3">
                               
                                <div class="  mb-3">
                                    <label for="product_size" class="form-label">Max Redeemable Amount</label>
                                    <input type="text" value="<%= coupon.maxRedeemableAmt %>" class="form-control" name="maxRedeemableAmt">                                                                   
                                    <div class="error"></div>
                                </div><br>
                            </div>
                            <div class="row gx-3">
                               
                                <div class="  mb-3">
                                    <label for="product_size" class="form-label">Expiry Date</label>
                                    <input type="text" value="<%= coupon.expiry%>" class="form-control" name="expiryDate" id="expiryDate">                                                                   
                                    <div class="error"></div>
                                </div>
                            </div>
                            
                           
                    </div>
                    <div style="margin:0 0 12px 21px";>
                           
                        <input type="submit" class="btn btn-md rounded font-sm hover-up" value="Add Coupon">
                    </div>
                </div>
            </div> <!-- card end// -->
