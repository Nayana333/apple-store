<body>
    <div class="screen-overlay"></div>
    <%- include("../partials/aside")%>
    
    <main class="main-wrap">
        
        <%- include("../partials/header2")%>
<table class="table align-middle table-nowrap mb-0 ms-4">
  
  <thead class="table-light"> 
  <tr>
                              
                                       
    <th class="align-middle" scope="col">COUPON CODE</th>                          
    <th class="align-middle" scope="col">DISCOUNT</th>
    <th class="align-middle" scope="col">LIMIT</th>
    
    <th class="align-middle" scope="col">EXPIRY DATE</th>
    <th class="align-middle" scope="col">LIST/UNLIST</th>
    <th class="align-middle" scope="col">EDIT</th>
   
</tr>
</thead>
<%  
if(coupon.length > 0){
  for(let i=0; i<coupon.length; i++){
%>
    <tr>
      <td><%= coupon[i].code %></td>
      <td><%= coupon[i].discount %>%</td>
      <td><%= coupon[i].limit %></td>
      
      <td>
        <% if (coupon[i].expiry) { %>
            <%= coupon[i].expiry.toDateString() %>
        <% } else { %>
            Expiry date not available
        <% } %>
    </td>
    <td> 
        <% if(coupon[i].isListed== true)
        {
          %>
            <p>Listed</p>
            <%

        }
        else{
          %>
            <p>Unlisted</p>
            <%

        }
        %></td>
        <td>
          <% if (coupon[i].isListed) { %>
              <a href="/admin/unlistCoupon?id=<%= coupon[i]._id.toString() %>">
               
                 <p class="text-danger ms-4">Unlist</p>
               
              </a>
            <% } else { %>
              <a href="/admin/unlistCoupon?id=<%= coupon[i]._id.toString() %>">
                
                  <p class="text-primary ms-4">List</p>
               
              </a>
            <% } %>
      </td>
      



    
      <td><a href="/admin/editCoupon?id=<%= coupon[i]._id.toString()%>" class="btn btn-xs">Edit</a></td>

    </tr>
<%
  }
}
%>

</table><br><br><br>

<!-- Your existing table code -->

<!-- Pagination logic -->
<% if (totalPages > 1) { %>
    <nav aria-label="Page navigation">
      <ul class="pagination justify-content-center">
        <% if (currentPage > 1) { %>
          <li class="page-item">
            <a class="page-link text-success" href="/admin/viewCoupon?page=<%= currentPage - 1 %>" aria-label="Previous">Previous</a>
          </li>
        <% } %>
  
        <% for (let page = 1; page <= totalPages; page++) { %>
          <li class="page-item <%= page === currentPage ? 'active' : '' %>">
            <a class="page-link text-success" href="/admin/viewCoupon?page=<%= page %>"><%= page %></a>
          </li>
        <% } %>
  
        <% if (currentPage < totalPages) { %>
          <li class="page-item">
            <a class="page-link text-success" href="/admin/viewCoupon?page=<%= currentPage + 1 %>" aria-label="Next">Next</a>
          </li>
        <% } %>
      </ul>
    </nav>
  <% } %>
  


