    <main class="main-wrap">
        
        <%- include("../partials/header2")%>

        <form method="get" action="">
            <select name="sortBy" class="form-control" style="position: relative; left: 3px; width: 186px">
                <option value="listed" <%= sortBy === "listed" ? "selected" : "" %>>Listed</option>
                <option value="unlist" <%= sortBy === "unlist" ? "selected" : "" %>>Unlisted</option>
            </select>
            <input type="submit" value="Apply" name="button">
        </form>
        

        
            <a href="#" data-bs-toggle="dropdown" class="btn btn-light rounded btn-sm font-sm">Sort </a>
            <div class="dropdown-menu">
                <a class="dropdown-item" href="#">Listed</a>
                <a class="dropdown-item" href="#">UnListed</a>
                </div>
        <table class="table align-middle table-nowrap mb-0 ms-4">
            <!-- Table header -->
            <thead class="table-light"> 
                <tr>                                   
                    <th class="align-middle" scope="col">IMAGE</th>                          
                    <th class="align-middle" scope="col">CATEGORY</th>
                    <th class="align-middle" scope="col">DESCRIPTION</th>
                    <th class="align-middle" scope="col">Listed Status</th>
                    <th class="align-middle" scope="col">LIST/UNLIST</th>
                    <th class="align-middle" scope="col">EDIT</th>
                </tr>
            </thead>
            <!-- Table body -->
            <tbody>
                <% if (categ.length > 0) { %>
                    <% for (let i = 0; i < categ.length; i++) { %>
                        <tr>
                            <td>
                                <img src="/public/categoryImages/<%= categ[i].image %>" style="width:100px" alt="hello">
                            </td>
                            <td><%= categ[i].category %></td>
                            <td style="width: 19%;"><%= categ[i].description %></td>
                            <td>
                                <% if (categ[i].isListed) { %>
                                    <p>Listed</p>
                                <% } else { %>
                                    <p>Unlisted</p>
                                <% } %>
                            </td>
                            <td>
                                <% if (categ[i].isListed) { %>
                                    <a href="/admin/unlistCategory?id=<%= categ[i]._id.toString() %>">
                                        <p class="text-danger ms-4">Unlist</p>
                                    </a>
                                <% } else { %>
                                    <a href="/admin/listCategory?id=<%= categ[i]._id.toString() %>">
                                        <p class="text-primary ms-4">List</p>
                                    </a>
                                <% } %>
                            </td>
                            <td>
                                <a href="/admin/editCategory?id=<%= categ[i]._id %>" class="btn btn-xs">Edit</a>
                            </td>
                        </tr>
                    <% } %>
                <% } else { %>
                    <!-- If no categories found -->
                    <tr>
                        <td colspan="6">Category not found</td>
                    </tr>
                <% } %>
            </tbody>
        </table>
        
        <!-- Pagination links -->
        <% if (totalPages > 1) { %>
            <div class="pagination">
                <% if (currentPage > 1) { %>
                    <a href="/admin/viewCategory?page=<%= currentPage - 1 %>">Previous</a>
                <% } %>
        
                <% for (let page = 1; page <= totalPages; page++) { %>
                    <a href="/admin/viewCategory?page=<%= page %>"><%= page %></a>
                <% } %>
        
                <% if (currentPage < totalPages) { %>
                    <a href="/admin/viewCategory?page=<%= currentPage + 1 %>">Next</a>
                <% } %>
            </div>
        <% } %>
</div>


