                                        ? 'checked' : '' %> style="width: 13px;height: 13px;">
                                        <label for="colorBlack">Black</label>
                                        <br>
                                        <input type="checkbox"  name="color" id="colorWhite" value="White"  <%=selectedColors.includes('White')
                                        ? 'checked' : '' %> style="width: 13px;height: 13px;">
                                        <label for="colorWhite">White</label>
                                        <br>
                                        <input type="checkbox"  name="color" id="colorBlue" value="Blue"<%=selectedColors.includes('Blue')
                                        ? 'checked' : '' %>  style="width: 13px;height: 13px;">
                                        <label for="colorBlue">Blue</label>
                                        <br>
                                        <input type="checkbox"  name="color" id="colorGray" value="Gray" <%=selectedColors.includes('Gray')
                                        ? 'checked' : '' %> style="width: 13px;height: 13px;">
                                        <label for="colorGray">Gray</label>
                                        <br>
                                        <input type="checkbox"  name="color" id="colorPink" value="Pink" <%=selectedColors.includes('Pink')
                                        ? 'checked' : '' %> style="width: 13px;height: 13px;">
                                        <label for="colorPink">Pink</label>
                                        <br>
                                   
                                </div>
                            </div>
                            <input  type="submit" value="filter">
                            <br>
                            <a href="/product" class="btn btn-sm btn-default"></i> Reset</a>
                        </div>
                    </form>
