        <section class="mt-50 mb-50">
            <form id="filterForm" action="" method="get">
            <div class="container">
                <div class="row flex-row-reverse">
                    <div class="col-lg-9">
                        <div class="shop-product-fillter">
                            <div class="totall-product"> 
                            </div>
                            <div class="sort-by-product-area">
                                <div class="sort-by-cover mr-10">
                                    
                                    
                                </div>
                                <form method="get" action=" " >
                                    <select name="sortBy" class="form-control" style="position: relative;
                                    left: 3px; width:186px">
                                
                                        <option value="priceLowToHigh"
                                            <%=sortBy==="priceLowToHigh" ? "selected" : "" %>
                                            >Price: Low to High</option>
                                        <option value="priceHighToLow"
                                            <%=sortBy==="priceHighToLow" ? "selected" : "" %>
                                            >Price: High to Low</option><br>
                                            <input type="submit" value="apply" name="button">
                            </select>

                        </form>
                            </div>
                        </div>
                        <div class="row product-grid-3">
                            <div class="row product-grid-3 col-lg-9" style="width: 100%;">
                 
                                <% if (product.length > 0) {
                                    for (let i = 0; i < product.length; i++) { %> 
                                <div class="col-lg-3 col-md-4 col-12 col-sm-6" >
                                    <% if (product[i].discountStatus && new Date(product[i].startDate) <= new Date() && new Date(product[i].endDate) >= new Date()) { %>
                                                                    
                                        <% if (product[i].price > product[i].discountPrice) { %>
                                            <span class="product-label label-top"></span>
                                        <% } %>
                                    <% } %>
                                    <div class="product-cart-wrap mb-30">
                                        <div class="product-img-action-wrap">
                                            <div class="product-img product-img-zoom">
                                                <a href="/aboutProduct?id=<%= product[i]._id.toString()%>">
                                                    <img class="default-img" src="/public/productImages/<%= product[i].productImages[3] %>" alt="">
                                                    <img class="hover-img" src="/public/productImages/<%= product[i].productImages[2] %>" alt="">
                                                </a>
                                            </div>
                                           
                                                <div class="product-action-1">
                                                    <a aria-label="Quick view" class="action-btn hover-up" href="/aboutProduct?id=<%= product[i]._id.toString()%>"><i class="fi-rs-eye"></i></a>
                                                     
                                                    <a aria-label="Add To Wishlist" class="action-btn hover-up" href="/addtoWishlist/<%=product[i]._id %>"><i class="fi-rs-heart"></i></a>
                                                   
                                                </div>
                                               
                                           
                                            <div class="product-badges product-badges-position product-badges-mrg">
                                                
                                            </div>
                                        </div>
                                        <div class="product-content-wrap">
                                            <div class="product-category">
                                                <a href="shop-grid-right.html"><%= product[i].category %></a>
                                            </div>
                                            <h2><a href="/aboutProduct?id=<%= product[i]._id.toString()%>"><%= product[i].name %></a></h2>
                                            <div class="rating-result" title="90%">
                                                <span>
                                                    <span>90%</span>
                                                </span>
                                            </div>
                                            <% if (product[i].quantity !== 0) { %>       
                                                <div class="product-price">
                                                    <% if (product[i].discountStatus && new Date(product[i].startDate) <= new Date() && new Date(product[i].endDate) >= new Date()) { %>
                                                        ₹ <%= product[i].discountPrice %>
                                                        <% if (product[i].price > product[i].discountPrice) { %>  
                                                            <span class="old-price ml-3" style="text-decoration: line-through;"> ₹ <%= product[i].price %> </span>
                                                    
                                                        <% } %>
                                                    <% } else { %>
                                                        ₹ <%= product[i].price %>
                                                    <% } %>
                                                </div><!-- End .product-price -->
                                           
                                            
                                            <div class="product-action">

                                                <form class="add-to-cart-form"
                                                    action="/addtoCart/<%= product[i]._id %>"
                                                    method="post">
                                                    <button  type="submit"   data-product-id="<%= product[i]._id %>" aria-label="Add to Cart" class="action-btn hover-up" style="height: 43px;
                                                        position: relative;left: 20px; width:-124px"><i class="fi-rs-shopping-bag-add"></i></button>
                                                 
                                                    <input type="number" value="1"
                                                        name="qty" style="display: none;">
                                                </form>
                                    </div>

                                    <% } else { %>
                                        <div class="my-3" style="display: flex; align-items: center; color: red;">
                                            <p style="color: red; font-size: 12px;">Product out of stock</p>
                                        </div>
                                    <% } %>
                                        </div>
                                    </div>
                                </div>
    
                                <% } 
                            } %>
                                </div>
                          
                        </div>
